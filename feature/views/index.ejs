<!doctype html>
<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" type="text/css" href="/static/style.css">
  <link href="https://fonts.googleapis.com/css?family=Hind&display=swap" rel="stylesheet">
  <title>Matchie</title>
</head>

<body>

<nav>
        <a href="/home/<%= _id %>"><img id="home" src="/static/images/logo-datingssite.png"></a>
  <ul>
    <li><a href="/boost/<%= _id %>">
        <img src="/static/images/boost.png">
            <div class="notification" id="noteBoost"><p class="number" id="valueBoost"><%= boost %></p></div>
        </a>
    </li>

    <li><a href="/likes/<%= _id %>">
        <img src="/static/images/likes.png">

            <div class="notification" id="noteLikes"><p class="number" id="valueLikes"><%= likes %></p></div>
          </a>
      </li>

    <li><a href="/messages/<%= _id %>">
        <img class="unactive" src="/static/images/messages.png">

            <div class="notification" id="noteMessages"><p class="number" id="valueMessages"><%= messages %></p></div>
          </a>
        </li>
  </ul>
</nav>

<main>
  <p>Welcome to Matchie!</p>
  <a href="/update/<%= _id %>" >Date up!</a>
</main>

<%- include("script.ejs") -%>
</body>
